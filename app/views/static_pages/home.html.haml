%section#contact.fullwidth.border-top.margin-top-40.margin-bottom-0.padding-top-60.padding-bottom-65{"data-background-color" => "#f9f9f9"}
  .container
    .row
      / Contact Details
      .col-md-4
        %h4.headline.margin-bottom-30= t(:contact_us)
        / Contact Details
        .sidebar-textbox
          %p= t(:slogan)
          %ul.contact-details
            %li
              %i.im.im-icon-Phone-2
              %strong #{t(:phone)}:
              %span= t(:phone_number)
            %li
              %i.fa.fa-wechat
              %strong #{t(:wechat)}:
              %span jiazhenjoe
            %li
              %i.im.im-icon-Globe
              %strong #{t(:web)}:
              %span
                %a{:href => "https://www.wondering.tech"} www.wondering.tech
            %li
              %i.im.im-icon-Envelope
              %strong #{t(:email)}:
              %span
                %a{:href => "#"}= t(:contact_email)
      / Contact Form
      .col-md-8
        %section#contact
          %h4.headline.margin-bottom-35= t(:contact_form)
          #contact-message
          = form_tag(contact_message_path) do
            = render "shared/flash_messages"
            .row
              .col-md-6
                %div
                  %input#name{:name => "contact[name]", :placeholder => t(:your_name), :required => "required", :type => "text"}/
              .col-md-6
                %div
                  %input#email{:name => "contact[email]", :pattern => "^[A-Za-z0-9](([_\\.\\-]?[a-zA-Z0-9]+)*)@([A-Za-z0-9]+)(([\\.\\-]?[a-zA-Z0-9]+)*)\\.([A-Za-z]{2,})$", :placeholder => t(:email_address), :required => "required", :type => "email"}/
            %div
              %input#subject{:name => "contact[subject]", :placeholder => t(:subject), :required => "required", :type => "text"}/
            %div
              %textarea#comments{:cols => "40", :name => "contact[message]", :placeholder => t(:message), :required => "required", :rows => "3", :spellcheck => "true"}
            %input#submit.submit.button{:type => "submit", :value => t(:submit_message)}/
      / Contact Form / End
  / Container / End


