%header#header-container
  / Header
  #header
    .container
      / Left Side Content
      .left-side
        / Logo
        #logo
          %a{:href => "index.html"}
            %img{:alt => "", :src => "images/logo.png"}/
        / Mobile Navigation
        .mmenu-trigger
          %button.hamburger.hamburger--collapse{:type => "button"}
            %span.hamburger-box
              %span.hamburger-inner
        / Main Navigation
        %nav#navigation.style-1
          %ul#responsive
            %li
              %a.current{:href => "#"} Home
              %ul
                %li
                  %a{:href => "index.html"} Home 1
                %li
                  %a{:href => "index-2.html"} Home 2
                %li
                  %a{:href => "index-3.html"} Home 3
                %li
                  %a{:href => "index-4.html"} Home 4
            %li
              %a{:href => "#"} Listings
              %ul
                %li
                  %a{:href => "#"} List Layout
                  %ul
                    %li
                      %a{:href => "listings-list-with-sidebar.html"} With Sidebar
                    %li
                      %a{:href => "listings-list-full-width.html"} Full Width
                %li
                  %a{:href => "#"} Grid Layout
                  %ul
                    %li
                      %a{:href => "listings-grid-with-sidebar-1.html"} With Sidebar 1
                    %li
                      %a{:href => "listings-grid-with-sidebar-2.html"} With Sidebar 2
                    %li
                      %a{:href => "listings-grid-full-width.html"} Full Width
                %li
                  %a{:href => "#"} Half Screen Map
                  %ul
                    %li
                      %a{:href => "listings-half-screen-map-list.html"} List Layout
                    %li
                      %a{:href => "listings-half-screen-map-grid-1.html"} Grid Layout 1
                    %li
                      %a{:href => "listings-half-screen-map-grid-2.html"} Grid Layout 2
                %li
                  %a{:href => "listings-single-page.html"} Single Listing
            %li
              %a{:href => "#"} User Panel
              %ul
                %li
                  %a{:href => "dashboard.html"} Dashboard
                %li
                  %a{:href => "dashboard-messages.html"} Messages
                %li
                  %a{:href => "dashboard-bookings.html"} Bookings
                %li
                  %a{:href => "dashboard-my-listings.html"} My Listings
                %li
                  %a{:href => "dashboard-reviews.html"} Reviews
                %li
                  %a{:href => "dashboard-bookmarks.html"} Bookmarks
                %li
                  %a{:href => "dashboard-add-listing.html"} Add Listing
                %li
                  %a{:href => "dashboard-my-profile.html"} My Profile
                %li
                  %a{:href => "dashboard-invoice.html"} Invoice
            %li
              %a{:href => "#"} Pages
              %ul
                %li
                  %a{:href => "pages-user-profile.html"} User Profile
                %li
                  %a{:href => "pages-booking.html"} Booking Page
                %li
                  %a{:href => "pages-blog.html"} Blog
                  %ul
                    %li
                      %a{:href => "pages-blog.html"} Blog
                    %li
                      %a{:href => "pages-blog-post.html"} Blog Post
                %li
                  %a{:href => "pages-contact.html"} Contact
                %li
                  %a{:href => "pages-coming-soon.html"} Coming Soon
                %li
                  %a{:href => "pages-elements.html"} Elements
                %li
                  %a{:href => "pages-pricing-tables.html"} Pricing Tables
                %li
                  %a{:href => "pages-typography.html"} Typography
